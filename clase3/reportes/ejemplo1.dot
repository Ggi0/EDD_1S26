digraph ListaCircular {
    // Configuracion general del grafo
    rankdir=LR;  // Orientacion horizontal (Left to Right)
    
    // Estilo de los nodos
    node [
        shape=circle,           // Forma circulo con divisiones
        style=filled,
        fillcolor=lightblue,
        fontsize=12,
        fontname="Arial"
    ];
    
    // Estilo de las flechas
    edge [
        color=darkgreen,
        penwidth=2,
        arrowsize=1.0
    ];

    // === NODOS DE LA LISTA ===
    node0 [
        label=" juan "
    ];
    node1 [
        label=" hola "
    ];
    node2 [
        label=" 7 "
    ];
    node3 [
        label=" 5 "
    ];
    node4 [
        label=" 4 "
    ];
    node5 [
        label=" 3 "
    ];
    node6 [
        label=" 2 "
    ];

    // === CONEXIONES ENTRE NODOS ===
    node0 -> node1;
    node1 -> node2;
    node2 -> node3;
    node3 -> node4;
    node4 -> node5;
    node5 -> node6;
    // Flecha que cierra el circulo (vuelve a HEAD)
    node6 -> node0 [
    ];
}
